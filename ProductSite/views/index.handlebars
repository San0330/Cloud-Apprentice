<div class='products'>
  {{#each products}}
    <div class='product'>
      <img src="{{image}}" />
      <div class='product-detail'>
        <h2>
          {{name}}
        </h2>
        <h2>
          {{price}}$
        </h2>
      </div>

      <p class='product-description'>
        {{description}}
      </p>

      <button class='order-btn'>
        Order
      </button>
    </div>
  {{/each}}
</div>
<script src='js/index.js'></script>